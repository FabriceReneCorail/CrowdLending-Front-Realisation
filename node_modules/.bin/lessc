#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/fabricerene-corail/Documents/ProjetFrontLendMe/AngularCrowdlundingG4/AngularCrowdlundingG4/node_modules/less/bin/lessc/node_modules:/Users/fabricerene-corail/Documents/ProjetFrontLendMe/AngularCrowdlundingG4/AngularCrowdlundingG4/node_modules/less/bin/node_modules:/Users/fabricerene-corail/Documents/ProjetFrontLendMe/AngularCrowdlundingG4/AngularCrowdlundingG4/node_modules/less/node_modules:/Users/fabricerene-corail/Documents/ProjetFrontLendMe/AngularCrowdlundingG4/AngularCrowdlundingG4/node_modules:/Users/fabricerene-corail/Documents/ProjetFrontLendMe/AngularCrowdlundingG4/node_modules:/Users/fabricerene-corail/Documents/ProjetFrontLendMe/node_modules:/Users/fabricerene-corail/Documents/node_modules:/Users/fabricerene-corail/node_modules:/Users/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../less/bin/lessc" "$@"
else 
  exec node  "$basedir/../less/bin/lessc" "$@"
fi
